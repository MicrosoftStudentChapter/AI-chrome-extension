{
  "manifest_version": 3,
  "name": "AI Content Detector",
  "version": "1.0",
  "description": "Detects AI-generated or biased content in webpages.",
  "permissions": ["activeTab", "scripting"],
  "host_permissions": ["<all_urls>"],

  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },

  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["detector.js", "content.js"],
      "css": ["style.css"]
    }
  ]
}