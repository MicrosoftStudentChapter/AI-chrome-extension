{
  "manifest_version": 3,
  "name": "AI Content Detector",
  "version": "1.0",
  "description": "Detects AI-generated or biased content in webpages.",
  "permissions": ["activeTab", "scripting"],
  "host_permissions": ["<all_urls>"],
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"],
      "css": ["style.css"]
    }
  ]
}